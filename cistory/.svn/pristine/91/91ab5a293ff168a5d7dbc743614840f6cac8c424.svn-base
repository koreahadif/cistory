package nhn.cistory.vo;

public class History {
	private int htr_num;
	private String htr_keyrd;
	private String htr_imguri;
	private String htr_desc;
	private String htr_tag;
	private String htr_type;
	private String htr_age;
	private String htr_date;
	public History(){
		
	}

	public History(int htr_num, String htr_keyrd, String htr_imguri,
			String htr_desc, String htr_tag, String htr_type, String htr_age, String htr_date) {
		super();
		this.htr_num = htr_num;
		this.htr_keyrd = htr_keyrd;
		this.htr_imguri = htr_imguri;
		this.htr_desc = htr_desc;
		this.htr_tag = htr_tag;
		this.htr_type = htr_type;
		this.htr_age = htr_age;
		this.htr_date = htr_date;
	}

	public int getHtr_num() {
		return htr_num;
	}

	public void setHtr_num(int htr_num) {
		this.htr_num = htr_num;
	}

	public String getHtr_keyrd() {
		return htr_keyrd;
	}

	public void setHtr_keyrd(String htr_keyrd) {
		this.htr_keyrd = htr_keyrd;
	}

	public String getHtr_imguri() {
		return htr_imguri;
	}

	public void setHtr_imguri(String htr_imguri) {
		this.htr_imguri = htr_imguri;
	}

	public String getHtr_desc() {
		return htr_desc;
	}

	public void setHtr_desc(String htr_desc) {
		this.htr_desc = htr_desc;
	}

	public String getHtr_tag() {
		return htr_tag;
	}

	public void setHtr_tag(String htr_tag) {
		this.htr_tag = htr_tag;
	}

	public String getHtr_type() {
		return htr_type;
	}

	public void setHtr_type(String htr_type) {
		this.htr_type = htr_type;
	}

	public String getHtr_age() {
		return htr_age;
	}

	public void setHtr_age(String htr_age) {
		this.htr_age = htr_age;
	}
	public String getHtr_date() {
		return htr_date;
	}

	public void setHtr_date(String htr_date) {
		this.htr_date = htr_date;
	}

	@Override
	public String toString() {
		return "History [htr_num=" + htr_num + ", htr_keyrd=" + htr_keyrd
				+ ", htr_imguri=" + htr_imguri + ", htr_desc=" + htr_desc
				+ ", htr_tag=" + htr_tag + ", htr_type=" + htr_type
				+ ", htr_age=" + htr_age + "]";
	}
}
