package nhn.cistory.vo;

public class Quiz extends History {
	private int quiz_num;
	private int htr_num ;
	private String quiz;
	private String quiz_sol;
	private String quiz_desc;

	public Quiz(){
	}

	public Quiz(int quiz_num, int htr_num, String quiz, String quiz_sol,
			String quiz_desc) {
		super();
		this.quiz_num = quiz_num;
		this.htr_num = htr_num;
		this.quiz = quiz;
		this.quiz_sol = quiz_sol;
		this.quiz_desc = quiz_desc;
	}

	public Quiz(int htr_num, String quiz, String quiz_sol, String quiz_desc) {
		super();
		this.htr_num = htr_num;
		this.quiz = quiz;
		this.quiz_sol = quiz_sol;
		this.quiz_desc = quiz_desc;
	}

	public int getQuiz_num() {
		return quiz_num;
	}

	public void setQuiz_num(int quiz_num) {
		this.quiz_num = quiz_num;
	}

	public int getHtr_num() {
		return htr_num;
	}

	public void setHtr_num(int htr_num) {
		this.htr_num = htr_num;
	}

	public String getQuiz() {
		return quiz;
	}

	public void setQuiz(String quiz) {
		this.quiz = quiz;
	}

	public String getQuiz_sol() {
		return quiz_sol;
	}

	public void setQuiz_sol(String quiz_sol) {
		this.quiz_sol = quiz_sol;
	}

	public String getQuiz_desc() {
		return quiz_desc;
	}

	public void setQuiz_desc(String quiz_desc) {
		this.quiz_desc = quiz_desc;
	}
	@Override
	public String toString() {
		return "Quiz [quiz_num=" + quiz_num + ", htr_num=" + htr_num
				+ ", quiz=" + quiz + ", quiz_sol=" + quiz_sol + ", quiz_desc="
				+ quiz_desc + "]";
	}
	
}
